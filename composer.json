{
    "name": "nwtn/php-respimg",
    "description": "A responsive image workflow for optimizing and resizing your images",
    "version": "1.0.1",
    "keywords": [
        "responsive images",
        "respimg",
        "responsive web design",
        "rwd",
        "imagemagick",
        "imagick",
        "resize"
    ],
    "homepage": "https://github.com/nwtn/php-respimg/",
    "license": "MIT",
    "authors": [
        {
            "name": "David Newton",
            "email": "david@davidnewton.ca"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nwtn/php-respimg/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-imagick": "*",
        "jonnyw/php-phantomjs": "3.1.5"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "nwtn\\": "src/",
            "": "src/"
        }
    }
}
